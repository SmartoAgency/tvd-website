  script(defer src='./assets/scripts/index.bundle.js')
  script(defer src='./assets/scripts/services.bundle.js')
block content
  .inner-front-screen
    .inner-front-screen__bg
      img(src="./assets/images/services/bg.jpg", alt="", srcset="")
    .inner-front-screen__content
      include ../includes/breadcrumbs.pug
      .inner-front-screen__title.text-style-1920-h-1.text-style-768-h-1.text-style-375-h-1.color-black #{title_page}
      +title-decor-inner-pages()
  .services-container
    each block in total
      .services-block
        .services-block__content
          .services-block__left
            .services-block__title-wrapper
              .services-block__title.text-style-1920-h-2.color-white #{block.title}
              .services-block__title-decor1
              .services-block__title-decor2
              .services-block__title-decor3
              .services-block__title-decor4
              .services-block__title-decor5
          .services-block__text(data-lenis-prevent)
            each val in block.items
              .services-block__accordeon
                .services-block__accordeon-top.ac-header
                  .services-block__accordeon-top-number.text-style-1920-h-3 #{val.numberSymbol}
                  .services-block__accordeon-top-title.text-style-1920-h-3 #{val.title}
                  .services-block__accordeon-top-icon
